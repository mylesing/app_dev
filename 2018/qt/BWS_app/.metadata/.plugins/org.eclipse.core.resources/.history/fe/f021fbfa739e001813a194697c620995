import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.Random;

public class Game {
	public int getGoal() {
		Random rand = new Random();
		return rand.nextInt(700) + 299;
	}
	
	public List<Integer> getNumbers(int bigs) {
		List<Integer> numbers = new ArrayList<Integer>;
		for(int i = 0; i < bigs; i++) {
			double x = Math.random();
			if (x > .75) {
				numbers.add(75);
			} else if ( x > .5) {
				numbers.add(50);
			} else {
				numbers.add(25);
			}
			
		}
		
		Random rand = new Random();
		for(int i = bigs; i < 7; i++) {
			int small = rand.nextInt(9) + 1;
			Arrays.binarySearch(numbers, key)
			numbers[i] = small;
		}
		return numbers;
	}
}
